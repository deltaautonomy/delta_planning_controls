<?xml version="1.0"?>
<package format="2">
  <name>delta_planning_controls</name>
  <version>1.0.0</version>
  <description>The delta_planning_controls package</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag -->
  <maintainer email="deltaautonomy@gmail.com">Delta Autonomy</maintainer>


  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <license>BSD</license>


  <!-- Url tags are optional, but multiple are allowed, one per tag -->
  <!-- Optional attribute type can be: website, bugtracker, or repository -->
  <url type="repository">https://github.com/deltaautonomy/delta_planning_controls</url>


  <!-- Author tags are optional, multiple are allowed, one per tag -->
  <!-- Authors do not have to be maintainers, but could be -->
  <author email="karmeshy@andrew.cmu.edu">Karmesh Yadav</author>


  <!-- The *depend tags are used to specify dependencies -->
  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>cmake_modules</build_depend>
  <build_depend>delta_msgs</build_depend>
  <build_depend>carla_ros_bridge</build_depend>
  <build_export_depend>delta_msgs</build_export_depend>
  <build_export_depend>carla_ros_bridge</build_export_depend>
  <exec_depend>delta_msgs</exec_depend>
  <exec_depend>carla_ros_bridge</exec_depend>
  <depend>rosparam_handler</depend>
  <depend>dynamic_reconfigure</depend>
  <depend>geometry_msgs</depend>


  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <!-- Other tools can request additional information be placed here -->

  </export>
</package>
